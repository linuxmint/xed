msgid "The selected color scheme cannot be installed."
msgstr "無法安裝所選的色彩配置。"

#: ../xed/dialogs/xed-preferences-dialog.c:812
msgid "Add Scheme"
msgstr "加入配色"

#: ../xed/dialogs/xed-preferences-dialog.c:819
msgid "A_dd Scheme"
msgstr "加入配色(_D)"

